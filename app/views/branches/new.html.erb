<!DOCTYPE html>
<html>
	<head>
		<title></title>
	</head>
	<body>
		<div class="branch_container">
			<div align="center">
				<%= render 'form',:branch => @branch %>
			</div>
      <br />
    <div align="center">
    <table width="30%" style="font-size:2%;" cellspacing="20">
    
      <% @branches.each do |branch| %>
      <tr style="font-size:20px;text-align:right;">
        <td width="5%"></td>
        <td width="15%" style="font-size:20px; text-align:left;"><%= branch.name %></td>
        <td width="5%" style="text-align:left;"><%= link_to image_tag('edit-icon.png'), edit_branch_path(branch) %></td>
        <td width="5%" style="text-align:left;"><%= link_to image_tag('Close-2-icon.png'), branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    </table>
    </div>
		</div>
	</body>
</html>
